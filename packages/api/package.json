{
  "name": "@blueserver/api",
  "description": "> server/client api provider(s)",
  "type": "module",
  "exports": {
    "./clients/http": {
      "types": "./exports/client/http/index",
      "import": "./exports/client/http/index.js"
    },
    "./clients/ws": {
      "types": "./exports/client/ws/index.d.ts",
      "import": "./exports/client/ws/index.js"
    },
    "./servers/http": {
      "import": "./exports/server/http.js",
      "types": "./exports/server/http.d.ts"
    },
    "./servers/ws": {
      "import": "./exports/server/ws.js",
      "types": "./exports/server/ws.d.ts"
    },
    ".": {
      "import": "./exports/api/api.js",
      "types": "./exports/api/api.d.ts"
    }
  },
  "scripts": {
		"build": "wireit",
		"serve": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"tsconfig.json",
				"src"
			],
			"output": [
				"exports"
			]
		}
	},
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "socket-request-client": "^1.7.5",
    "socket-request-server": "^1.6.6"
  },
  "devDependencies": {
    "@blueserver/server": "*"
  }
}
